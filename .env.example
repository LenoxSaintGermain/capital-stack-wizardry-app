# Supabase Configuration
# Copy this file to .env and replace with your actual values

# Your Supabase Project URL (found in Project Settings > API)
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase Anon/Public API Key (found in Project Settings > API)
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Your Supabase Service Role Key (found in Project Settings > API) - Keep this secret!
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# For Supabase CLI (optional - you can also use supabase login)
SUPABASE_ACCESS_TOKEN=your_access_token_here

# Database URL (for direct database access if needed)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.your-project-id.supabase.co:5432/postgres

# Project Reference ID
SUPABASE_PROJECT_ID=your_project_id

# OAuth Configuration
# Site URL and redirect URLs (configure these in Supabase Auth settings)
VITE_SITE_URL=http://localhost:5173
VITE_REDIRECT_URL=http://localhost:5173/auth/callback

# GitHub OAuth (if you need client ID for custom implementation)
# VITE_GITHUB_CLIENT_ID=your_github_client_id_here
